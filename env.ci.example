# CI/CD Environment Variables
# Use these variables in your CI/CD pipeline

# Supabase Configuration (CI/CD)
VITE_SUPABASE_URL=${SUPABASE_URL}
VITE_SUPABASE_ANON_KEY=${SUPABASE_ANON_KEY}

# Voice Chat Settings (CI/CD)
VITE_VOICE_CHAT_MAX_PARTICIPANTS=20
VITE_VOICE_CHAT_DEFAULT_LANGUAGE=en
VITE_VOICE_CHAT_AUDIO_QUALITY=medium

# CI/CD Settings
VITE_DEV_MODE=false
VITE_ENABLE_LOGGING=false

