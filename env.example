# Supabase Configuration for Voice Chat
# Copy this file to .env.local and fill in your actual values

# Supabase Project URL (required)
VITE_SUPABASE_URL=https://your-project-id.supabase.co

# Supabase Anonymous Key (required)
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Voice Chat Configuration (optional - defaults will be used)
VITE_VOICE_CHAT_MAX_PARTICIPANTS=20
VITE_VOICE_CHAT_DEFAULT_LANGUAGE=en
VITE_VOICE_CHAT_AUDIO_QUALITY=high

# Development Settings (optional)
VITE_DEV_MODE=false
VITE_ENABLE_LOGGING=true

# Example values (replace with your actual Supabase credentials):
# VITE_SUPABASE_URL=https://abcdefghijklmnop.supabase.co
# VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6ImFiY2RlZmdoaWprbG1ub3AiLCJyb2xlIjoiYW5vbiIsImlhdCI6MTYzNjU2NzI5MCwiZXhwIjoxOTUyMTQzMjkwfQ.example_signature_here 